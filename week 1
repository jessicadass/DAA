#include<stdio.h>
int main()
{
int t,j=0,n,i,num,flag=0;
printf("enter the number of test cases \n");
scanf("%d",&t);
while(j<t)
 {
   printf("enter the value of n \n");
   scanf("%d",&n);
   flag=0;
   int a[n];
   for(i=0;i<n;i++)
    {
      printf("enter element \n");
      scanf("%d",&a[i]);
    }
    printf("enter number to be searched \n");
    scanf("%d",&num);
    i=0;
    while(i<n)
      {
        if(a[i]==num)
          {
            printf("element present at %d\n",i+1);
            flag=1;
            break;
          }
      i++;
      }
if(flag==0)
printf("element not found");
j++;
}
}
